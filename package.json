{
  "name": "dble-layout",
  "version": "0.0.14",
  "private": false,
  "files": [
    "dist",
    "README.md"
  ],
  "main": "dist/package/index.js",
  "module": "dist/package/index.js",
  "types": "dist/package/index.d.ts",
  "browser": "./browser/specific/main.js",
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "@emotion/css": "^11.13.5",
    "@emotion/react": "^11.14.0",
    "@testing-library/jest-dom": "^6.3.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.6",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-flatlist-ui": "^1.1.4",
    "react-scripts": "5.0.1",
    "rollup-plugin-visualizer": "^5.13.1",
    "typescript": "^4.9.5",
    "vite": "^5.0.12",
    "web-vitals": "^3.5.1"
  },
  "exports": {
    ".": {
      "import": "./dist/package/index.js",
      "require": "./dist/package/index.js",
      "default": "./dist/package/index.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "clean": "rimraf dist",
    "compile": "npm run clean && cross-env NODE_ENV=production babel src/lib --out-dir dist --copy-files",
    "start": "react-scripts start",
    "build": "tsc && vite build --base=./",
    "build:cjs": "babel src --out-dir dist/cjs --copy-files --env-name cjs",
    "build:esm": "babel src --out-dir dist/esm --copy-files --env-name esm",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prepare": "rimraf dist && mkdir dist && tsc"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "description": "dble layout component library",
  "repository": {
    "type": "git",
    "url": "https://github.com/deep-hwan/dble-layout"
  },
  "keywords": [
    "dble",
    "typescript",
    "Reactjs",
    "react-component",
    "nextjs",
    "widget",
    "design-component"
  ],
  "author": "deep",
  "license": "ISC",
  "homepage": "https://www.dbleagency.com",
  "devDependencies": {
    "vite-plugin-dts": "^3.7.2"
  }
}
